; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "WindowDialog",
"class": "AbstractFormDialog",
"language": "GDScript",
"path": "res://addons/cardengine/ui/dialogs/abstract_form_dialog/abstract_form_dialog.gd"
}, {
"base": "Node",
"class": "AbstractManager",
"language": "GDScript",
"path": "res://addons/cardengine/core/abstract_manager.gd"
}, {
"base": "Reference",
"class": "CardData",
"language": "GDScript",
"path": "res://addons/cardengine/core/card.gd"
}, {
"base": "Reference",
"class": "CardDatabase",
"language": "GDScript",
"path": "res://addons/cardengine/core/database.gd"
}, {
"base": "Node",
"class": "CardEngineInstance",
"language": "GDScript",
"path": "res://addons/cardengine/core/card_engine_instance.gd"
}, {
"base": "TabContainer",
"class": "CardEngineUI",
"language": "GDScript",
"path": "res://addons/cardengine/ui/card_engine_ui.gd"
}, {
"base": "AbstractManager",
"class": "DatabaseManager",
"language": "GDScript",
"path": "res://addons/cardengine/core/database_manager.gd"
}, {
"base": "AbstractManager",
"class": "GeneralManager",
"language": "GDScript",
"path": "res://addons/cardengine/core/general_manager.gd"
}, {
"base": "Node",
"class": "UtilsInstance",
"language": "GDScript",
"path": "res://addons/cardengine/core/utils.gd"
} ]
_global_script_class_icons={
"AbstractFormDialog": "",
"AbstractManager": "",
"CardData": "",
"CardDatabase": "",
"CardEngineInstance": "",
"CardEngineUI": "",
"DatabaseManager": "",
"GeneralManager": "",
"UtilsInstance": ""
}

[application]

config/name="CardEngine"
run/main_scene="res://main.tscn"
config/icon="res://icon.png"

[autoload]

Utils="*res://addons/cardengine/core/utils.gd"
CardEngine="*res://addons/cardengine/core/card_engine_instance.gd"

[display]

window/size/width=1280
window/size/height=720

[editor_plugins]

enabled=PoolStringArray( "cardengine", "godot-plugin-refresher" )

[gui]

timers/tooltip_delay_sec=0.1

[rendering]

quality/2d/use_pixel_snap=true
quality/filters/use_nearest_mipmap_filter=true
environment/default_clear_color=Color( 0, 0, 0, 1 )
quality/filters/msaa=3
environment/default_environment="res://default_env.tres"
